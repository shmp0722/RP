%% feTrack_test.m

%% set directory
  [homeDir,subDir,JMD,CRD,LHON,Ctl,RP] = Tama_subj2;

%%
 for i = 1:length(subDir);
        
    SubDir = fullfile(homeDir,subDir{i});
    fgDir  = fullfile(SubDir,'/dwi_2nd/fibers/life_mrtrix');
%     roiDir = fullfile(SubDir,'/dwi_2nd/ROIs');
    dtDir  = fullfile(SubDir,'dwi_2nd');
    dt  = fullfile(dtDir,'dt6.mat');
     
    %% feTrack
    [status, results, fg, pathstr] = feTrack(trackingAlgorithm, dtFile,fibersFolder,lmax,nSeeds,wmMask)
    
    [status, results, fg, pathstr] = SO_feTrack('prob',dt,fgDir,2,500000,[]);
 end